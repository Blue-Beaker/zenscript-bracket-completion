{
  "name": "zsbc",
  "displayName": "Zenscript Brackethandler Completion",
  "description": "Zenscript bracket handlers completion, using data dumped from a custom crafttweaker script.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:mcfunction",
    "onLanguage:zenscript",
    "onLanguage:javascript",
    "onLanguage:typescript"],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Zenscript Brackethandler Completion",
      "order": 21,
      "properties": {
        "zsbc.path": {
          "type": "string",
          "markdownDescription": "Path to your crafttweaker.log",
          "scope": "window"
        },
        "zsbc.cache_path": {
          "type": "string",
          "markdownDescription": "Path to store caches of ZSBC so there is no need to read from crafttweaker.log every time.",
          "scope": "window"
        },
        "zsbc.ctapi": {
          "type": "string",
          "default":"mc1.12.x",
          "enum": ["mc1.12.x"],
          "markdownDescription": "Crafttweaker version.",
          "scope": "window"
        }
      }
    },
    "commands": [
			{
				"command": "zsbc.reload",
				"title": "ZenScript Completion: Reload data"
			},
      {
        "command": "zsbc.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
